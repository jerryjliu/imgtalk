{
	"manifest_version": 2,
	"name": "ImageTalk",
	"description": "Will give automatically generated audio descriptions of images.",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"background": {
		"scripts": ["eventPage.js", "jquery-2.2.4.min.js"],
		"persistent": true
	},

	"permissions": [
		"activeTab",
		"contextMenus"
	]
}